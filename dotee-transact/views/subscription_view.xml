<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="view_users_form_extends" model="ir.ui.view">
			<field name="name">	account.move.form</field>
			<field name="model">sale.subscription</field>
			<field name="inherit_id" ref="account.view_move_form"/>
			<field name="arch" type="xml">
				<xpath expr="//form[1]/sheet[1]/group[1]/group[1]/field[@name='in_progress']" position="after">
					<field name="period" string="Number of months"/>
				</xpath>
				<xpath expr="//form[1]/sheet[1]/div[1]/notebook[1]/page[1]/div/field[@name='recurring_invoice_line_ids']/tree[1]/field[@name='quantity']" position="after">
					<field name="unit_vacant" string="Vacant"/>
				</xpath>
			</field>
		</record>
	</data>
</odoo>